/*订单列表*/
.orderDetails{
	background: #fff;
	border-bottom: 20rpx solid #f3f3f3;
}
.orderListTitle{
	height: 100rpx;
	line-height: 100rpx;
	border-bottom: 1rpx solid #f3f3f3;
}
.orderListTitle .userImg{
	float: left;
	width: 70rpx;
	height: 70rpx;
	border-radius: 35rpx;
	margin:15rpx;
}
.orderListTitle .userImg image{
	border-radius: 35rpx;
	width: 100%;
	height: 100%;
}
.orderListTitle .userName{
	padding-right: 50rpx;
	background-size: 35rpx 35rpx;
}
.orderListTitle .orderStatus{
	float: right;
	margin-right: 20rpx;
	color: #2f7b27;
	font-size: 34rpx;
}

.state{
  float: right;
  margin-right: 20rpx;
  color: black;
}

.totalprice{
  font-size: 16px;
  color: red;
}


.allLast {
  margin-top: 10rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  color: #999;
  font-size: 28rpx;
  border-bottom: 1rpx solid #f3f3f3;
}

.allLast navigator {
  color: #333;
  width: 120rpx;
  text-align: center;
  display: inline-block;
  border: 2rpx #ddd solid;
  padding: 14rpx 10rpx;
  border-radius: 6rpx;
}
.allLast .continue{
  float: left;
  margin-left: 90px;
}
.allLast .bg_y {
  background: #ffe400;
  border: 2rpx #ffe400 solid;
  margin-left: 10rpx;
}

.orderListDetails{
	display: flex;
	height: 180rpx;
	border-bottom: 1rpx solid #f3f3f3;
}
.orderListDetails .productImg image{
	width: 100%;
	height: 100%;
}
.orderListDetails .productInfo{
	flex: 4;
	padding: 20rpx 30rpx;
}

.orderListDetails .productInfo .odatetime{
	float: right;
}
.productCount{
	height: 80rpx;
	line-height: 80rpx;
	border-bottom: 1rpx solid #f3f3f3;
}
.productCount>view{
	float: right;
	margin-right: 30rpx;
}




.tab {
  display: -ms-flexbox;
  display: flex;
  width: 100%;
  height: 40px;
  line-height: 40px;
  /* border: 1px solid rgba(7,17,27,0.1); */
  position: relative;
}

.tab .tab-item {
    -ms-flex: 1;
    flex: 1;
    text-align: center;
}

.tab .tab-item a {
  display: block;
  font-size: 14px;
  color: #4d555d;
}

.tab .tab-item .active {
  color: #f01414;
}





